  "description": "Jean Tinland's design system (reusable components collection)",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.0",
    "@babel/core": "^7.12.0",
    "@babel/plugin-proposal-class-properties": "^7.12.0",
    "@babel/preset-react": "^7.12.0",
    "@babel/preset-typescript": "^7.12.0",
    "@types/css-modules": "^1.0.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.4",
    "classnames": "^2.3.1",
    "concurrently": "^7.2.1",
    "cpx": "^1.5.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "clean": "rm -rf es",
    "build": "babel src -x '.js,.ts,.tsx' -d es && tsc",
    "postbuild": "cpx 'src/**/*.{json,css}' es",
    "prepublishOnly": "yarn clean && yarn build",
    "watch": "yarn clean && babel ./src -x '.js,.ts,.tsx' -d ./es -w & cpx 'src/**/*.{json,css}' ./es -w"
  },
  "packageManager": "yarn@1.22.18",
  "files": [
    "es",
    "button.js",
    "checkbox.js",
    "icons.js",
    "input.js",
    "multiple-select.js",
    "number-picker.js",
    "select.js",
    "snackbar.js",
    "textarea.js",
    "tooltip.js"
  ],
  "browserslist": [
    ">1%",
    "not dead",
    "ie >= 11",
    "not op_mini all"
  ]
}
